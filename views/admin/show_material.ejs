<%- include('header.ejs') -%>
<!-- Form Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="row g-4">

                        <div class="bg-secondary rounded h-100 p-4">
                            <h6 class="mb-4"><div class="abc">Material</div> </h6>
                            <% if(data.length) { %>
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th scope="col">#</th>
                                        <th scope="col">title</th>
                                        <th scope="col">Course</th>
                                        <th scope="col">Semester</th>
                                       
                                        <th scope="col">Action</th>
                                        <th>Download Material&nbsp;&nbsp;<i class="fa fa-download"></i></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for(var i = 0; i< data.length; i++) { %>
                                    <tr>
                                        <th scope="row"><%= (i+1) %></th>
                                        <td><%= data[i].title%></td>
                                    
                                        <td><%= data[i].course%></td>
                                        <td><%= data[i].sem%></td>
                                    
                                           <td>
                                            <a class="btn btn-outline-success edit" href="../admin/medit/<%=data[i].id%>">Edit</a>
                                            <a class="btn btn-outline-danger delete" onclick="return confirm('Are you sure want to delete this record?')" href="/admin/mdelete/<%=data[i].id%>">Delete</a> 
                                        </td>
                                        <td><a href='<%= "../"+data[i].photo %>' class="btn btn-outline-success" download="newname"><i class="fa fa-download"></i>&nbsp;&nbsp; download here</a></td>
               
                                    </tr>
                                   <% } %>
                                </tbody>
                            </table>
                            <% } %>
                            <% if(!data.length) { %>
                                <p class="text-center">No users found!</p>
                            <% } %>
                        </div>
                    

      
                    <%- include('footer.ejs') -%>