<%- include('header.ejs') -%>
<!-- Form Start -->
  <!-- Header Start -->
  <div class="container-fluid page-header" style="margin-bottom: 90px;">
    <div class="container">
        <div class="d-flex flex-column justify-content-center" style="min-height: 300px">
            <h3 class="display-4 text-white text-uppercase">Your Material</h3>
            <div class="d-inline-flex text-white">
                <p class="m-0 text-uppercase"><a  class="text-white" href="/user">Home</a></p>
                <i class="fa fa-angle-double-right pt-1 px-3"></i>
                <p class="m-0 text-uppercase">Your Material</p>
            </div>
        </div>
    </div>
</div>
<!-- Header End -->

      

                        <div class="bg-secondary rounded h-100 p-4">
                            <h6 class="mb-4"><div class="abc">Material</div> </h6>
                            <% if(data.length) { %>
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th scope="col">#</th>
                                        <th scope="col">Subjects</th>
                                        <th scope="col">Course</th>
                                        <th scope="col">Semester</th>
                                       
                                
                                        <th>Download Material&nbsp;&nbsp;<i class="fa fa-download"></i></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for(var i = 0; i< data.length; i++) { %>
                                    <tr>
                                        <th scope="row"><%= (i+1) %></th>
                                        <td><%= data[i].title%></td>
                                    
                                        <td><%= data[i].course%></td>
                                        <td><%= data[i].sem%></td>
                                    
                                        <td><a href='<%= "../"+data[i].photo %>' class="btn btn-outline-success" download="newname"><i class="fa fa-download"></i>&nbsp;&nbsp; download here</a></td>
               
                                    </tr>
                                   <% } %>
                                </tbody>
                            </table>
                            <% } %>
                            <% if(!data.length) { %>
                                <p class="text-center">No users found!</p>
                            <% } %>
                        </div>
                    

      
                    <%- include('footer.ejs') -%>